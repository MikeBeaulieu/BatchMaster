{
	"name": "jsonix-schema-compiler-sample-po-npm",
	"version": "2.3.9",
	"engines": [
		"node >= 0.8.0"
	],
	"main": "src/main/javascript/index.js",
	"dependencies": {
		"jsonix": "x.x.x",
		"jsonix-schema-compiler": "x.x.x"
	},
	"devDependencies": {
		"nodeunit": "~0.8.6"
	},
	"scripts": {
		"prepublish": "java -jar node_modules/jsonix-schema-compiler/lib/jsonix-schema-compiler-full.jar -d target/generated-sources/xjc src/main/resources/purchaseorder.xsd -b src/main/resources/bindings.xjb",
		"generatenz": "java -jar node_modules/jsonix-schema-compiler/lib/jsonix-schema-compiler-full.jar -d target/generated-sources/xjc src/main/resources/nz-customized-schema-4.xsd -b src/main/resources/bindings-nz.xjb",
		"generateaustrac": "java -jar node_modules/jsonix-schema-compiler/lib/jsonix-schema-compiler-full.jar -d target/generated-sources/xjc src/main/resources/FIRMA-IFTI-DRA-1-2.xsd -b src/main/resources/bindings-austrac.xjb",
		"test": "nodeunit src/test/javascript/tests.js"
	}
}
