<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns="http://austrac.gov.au/schema/reporting/IFTI-DRA-1-2"
           xmlns:xlink="http://www.w3.org/1999/xlink"
           xmlns:d="http://docbook.org/ns/docbook"
           targetNamespace="http://austrac.gov.au/schema/reporting/IFTI-DRA-1-2"
           elementFormDefault="qualified"
           xml:lang="en">
   <xs:annotation>
      <xs:documentation source="titlePage">
         <d:title>Electronic report file format specification – 
        international funds transfer instruction under a
        designated remittance arrangement</d:title>
         <d:author>
            <d:orgname>Australian Transaction Reports and Analysis Centre (AUSTRAC)</d:orgname>
            <d:uri>http://www.austrac.gov.au/</d:uri>
         </d:author>
         <d:revhistory>
            <d:revision>
               <d:revnumber>1.2</d:revnumber>
               <d:date>July 2010</d:date>
            </d:revision>
         </d:revhistory>
         <d:copyright>
            <d:year>2010</d:year>
            <d:holder>Australian Transaction Reports and Analysis Centre (AUSTRAC)</d:holder>
         </d:copyright>
         <d:keywordset>
            <d:keyword>AUSTRAC</d:keyword>
            <d:keyword>IFTI-DRA</d:keyword>
            <d:keyword>International funds transfer instruction under a designated remittance arrangement</d:keyword>
            <d:keyword>Report file format</d:keyword>
            <d:keyword>User guide</d:keyword>
            <d:keyword>Schema reference</d:keyword>
            <d:keyword>XSD reference</d:keyword>
         </d:keywordset>
      </xs:documentation>
   </xs:annotation>
   <xs:element name="ifti-draList">
      <xs:complexType>
         <xs:sequence>
            <xs:element name="reNumber" type="RENumber"/>
            <xs:element name="fileName" type="IFTIDRAFileName"/>
            <xs:element name="reportCount" type="ReportCount"/>
            <xs:element ref="ifti-dra" maxOccurs="unbounded"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="ifti-dra">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="header" minOccurs="0"/>
            <xs:element ref="transaction"/>
            <xs:element ref="transferor" maxOccurs="unbounded"/>
            <xs:element ref="orderingInstn"/>
            <xs:element ref="initiatingInstn" minOccurs="0"/>
            <xs:element ref="sendingInstn"/>
            <xs:element ref="receivingInstn"/>
            <xs:element ref="beneficiaryInstn"/>
            <xs:element ref="beneficiaryBranch" minOccurs="0"/>
            <xs:element ref="transferee" maxOccurs="unbounded"/>
            <xs:element ref="additionalDetails" minOccurs="0"/>
         </xs:sequence>
         <xs:attributeGroup ref="RequiredAttrs"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="header">
      <xs:complexType>
         <xs:sequence>
            <xs:element name="txnRefNo" type="TRN" minOccurs="0"/>
			<xs:element name="interceptFlag" type="xs:token" fixed="Y" minOccurs="0"/>
         </xs:sequence>
         <xs:attributeGroup ref="RequiredAttrs"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="transaction">
      <xs:complexType>
         <xs:sequence>
            <xs:element name="txnDate" type="AUSTRACDate" minOccurs="1"/>
            <xs:element name="currencyAmount" type="CurrencyAmount" minOccurs="1"/>
            <xs:element name="direction" type="Direction" minOccurs="1"/>
            <xs:element name="tfrType" type="TfrType" minOccurs="1"/>
            <xs:element name="valueDate" type="AUSTRACDate" minOccurs="0"/>
         </xs:sequence>
         <xs:attributeGroup ref="RequiredAttrs"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="transferor">
      <xs:complexType>
         <xs:sequence>
            <xs:element name="fullName" type="Name" minOccurs="1"/>
            <xs:element name="altName" type="Name" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="mainAddress" type="Address" minOccurs="1"/>
            <xs:element name="postalAddress" type="Address" minOccurs="0"/>
            <xs:element name="phone" type="PhoneNum" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="email" type="Email" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="indOcc" type="IndustryOccupation" minOccurs="0"/>
            <xs:element name="abn" type="ABN" minOccurs="0"/>
            <xs:element name="acn" type="ACN" minOccurs="0"/>
            <xs:element name="arbn" type="ARBN" minOccurs="0"/>
            <xs:element name="custNo" type="CustNumber" minOccurs="1"/>
            <xs:element name="acctNumber" type="AcctNumber" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="businessStruct" type="BusinessStructure" minOccurs="0"/>
            <xs:element name="dob" type="DateOfBirth" minOccurs="0"/>
            <xs:element name="identification" type="IdentificationNoCountry" minOccurs="0"
                        maxOccurs="unbounded"/>
            <xs:element name="electDataSrc" type="ElectronicDataSource" minOccurs="0"
                        maxOccurs="unbounded"/>
         </xs:sequence>
         <xs:attributeGroup ref="RequiredAttrs"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="orderingInstn">
      <xs:complexType>
         <xs:sequence>
            <xs:element name="branch" type="BranchOptCountryFirma" minOccurs="0"/>
            <xs:element name="foreignBased" type="YesNo" minOccurs="1"/>
            <xs:element name="altName" type="Name" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="postalAddress" type="Address" minOccurs="0"/>
            <xs:element name="phone" type="PhoneNum" minOccurs="0"/>
            <xs:element name="email" type="Email" minOccurs="0"/>
            <xs:element name="indOcc" type="IndustryOccupation" minOccurs="0"/>
            <xs:element name="businessStruct" type="BusinessStructure" minOccurs="0"/>
            <xs:element name="dob" type="DateOfBirth" minOccurs="0"/>
         </xs:sequence>
         <xs:attributeGroup ref="RequiredAttrs"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="initiatingInstn">
      <xs:complexType>
         <xs:sequence>
            <xs:element name="sameAsOrderingInstn" fixed="Y" type="xs:token" minOccurs="1"/>
         </xs:sequence>
         <xs:attributeGroup ref="RequiredAttrs"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="sendingInstn">
      <xs:complexType>
         <xs:sequence>
            <xs:element name="sameAsOrderingInstn" fixed="Y" type="xs:token" minOccurs="1"/>
         </xs:sequence>
         <xs:attributeGroup ref="RequiredAttrs"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="receivingInstn">
      <xs:complexType>
         <xs:sequence>
            <xs:element name="fullName" type="Name" minOccurs="1"/>
            <xs:element name="mainAddress" type="Address" minOccurs="1"/>
         </xs:sequence>
         <xs:attributeGroup ref="RequiredAttrs"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="beneficiaryInstn">
      <xs:complexType>
         <xs:choice>
            <xs:element name="sameAsReceivingInstn" fixed="Y" type="xs:token"/>
            <xs:sequence>
               <xs:element name="fullName" type="Name" minOccurs="1"/>
               <xs:element name="mainAddress" type="AddressBeneficiaryInstAndTransferee" minOccurs="1"/>
            </xs:sequence>
         </xs:choice>
         <xs:attributeGroup ref="RequiredAttrs"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="beneficiaryBranch">
      <xs:complexType>
         <xs:sequence>
            <xs:element name="branch" type="BranchOptCountry"/>
         </xs:sequence>
         <xs:attributeGroup ref="RequiredAttrs"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="transferee">
      <xs:complexType>
         <xs:sequence>
            <xs:element name="fullName" type="Name"/>
            <xs:element name="altName" type="Name" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="mainAddress" type="AddressBeneficiaryInstAndTransferee" minOccurs="1"/>
            <xs:element name="postalAddress" type="AddressBeneficiaryInstAndTransferee" minOccurs="0"/>
            <xs:element name="phone" type="PhoneNum" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="email" type="Email" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="indOcc" type="IndustryOccupation" minOccurs="0"/>
            <xs:element name="abn" type="ABN" minOccurs="0"/>
            <xs:element name="acn" type="ACN" minOccurs="0"/>
            <xs:element name="arbn" type="ARBN" minOccurs="0"/>
            <xs:element name="account" type="InstitutionWithAccount" minOccurs="1"
                        maxOccurs="unbounded"/>
            <xs:element name="businessStruct" type="BusinessStructure" minOccurs="0"/>
            <xs:element name="dob" type="DateOfBirth" minOccurs="0"/>
         </xs:sequence>
         <xs:attributeGroup ref="RequiredAttrs"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="additionalDetails">
      <xs:complexType>
         <xs:sequence>
            <xs:element name="reasonForTransfer" type="ReasonForTransfer"/>
         </xs:sequence>
         <xs:attributeGroup ref="RequiredAttrs"/>
      </xs:complexType>
   </xs:element>
   <xs:attributeGroup xmlns:xhtml="http://www.w3.org/1999/xhtml" name="RequiredAttrs">
      <xs:attribute name="id" type="xs:ID" use="required"/>
   </xs:attributeGroup>
   <xs:attributeGroup xmlns:xhtml="http://www.w3.org/1999/xhtml" name="OptionalAttrs">
      <xs:attribute name="id" type="xs:ID"/>
   </xs:attributeGroup>
   <xs:simpleType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="RENumber">
      <xs:restriction base="xs:token">
         <xs:pattern value="[0-9]{1,7}"/>
		 <xs:enumeration value="24497"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="IFTIDRAFileName">
      <xs:restriction base="xs:token">
         <xs:pattern value="[iI][fF][tT][iI]\-[dD][rR][aA]20[0-9][0-9](0[1-9]|1[0-2])(0[1-9]|[1-2][0-9]|3[0-1])[0-9][0-9]\.[xX][mM][lL]"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="IFTIEFileName">
      <xs:restriction base="xs:token">
         <xs:pattern value="[iI][fF][tT][iI]\-[eE]20[0-9][0-9](0[1-9]|1[0-2])(0[1-9]|[1-2][0-9]|3[0-1])[0-9][0-9]\.[xX][mM][lL]"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="SMRFileName">
      <xs:restriction base="xs:token">
         <xs:pattern value="[sS][mM][rR]20[0-9][0-9](0[1-9]|1[0-2])(0[1-9]|[1-2][0-9]|3[0-1])[0-9][0-9]\.[xX][mM][lL]"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="TTRFBSFileName">
      <xs:restriction base="xs:token">
         <xs:pattern value="[tT][tT][rR]\-[fF][bB][sS]20[0-9][0-9](0[1-9]|1[0-2])(0[1-9]|[1-2][0-9]|3[0-1])[0-9][0-9]\.[xX][mM][lL]"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="TTRGSFileName">
      <xs:restriction base="xs:token">
         <xs:pattern value="[tT][tT][rR]\-[gG][sS]20[0-9][0-9](0[1-9]|1[0-2])(0[1-9]|[1-2][0-9]|3[0-1])[0-9][0-9]\.[xX][mM][lL]"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="TTRISIFileName">
      <xs:restriction base="xs:token">
         <xs:pattern value="[tT][tT][rR]\-[iI][sS][iI]20[0-9][0-9](0[1-9]|1[0-2])(0[1-9]|[1-2][0-9]|3[0-1])[0-9][0-9]\.[xX][mM][lL]"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="TTRMSBFileName">
      <xs:restriction base="xs:token">
         <xs:pattern value="[tT][tT][rR]\-[mM][sS][bB]20[0-9][0-9](0[1-9]|1[0-2])(0[1-9]|[1-2][0-9]|3[0-1])[0-9][0-9]\.[xX][mM][lL]"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="ReportCount">
      <xs:restriction base="xs:positiveInteger">
         <xs:totalDigits value="6"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="ReportType">
      <xs:restriction base="xs:token">
         <xs:enumeration value="IFTI-E"/>
         <xs:enumeration value="IFTI-DRA"/>
         <xs:enumeration value="TTR-MSB"/>
         <xs:enumeration value="TTR-GS"/>
         <xs:enumeration value="TTR-ISI"/>
         <xs:enumeration value="TTR-FBS"/>
         <xs:enumeration value="SMR"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="Name">
      <xs:restriction base="xs:string">
	     <xs:minLength value="1"/>
         <xs:maxLength value="140"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="NameType">
      <xs:restriction base="xs:token">
         <xs:enumeration value="M"/>
         <xs:enumeration value="A"/>
         <xs:enumeration value="T"/>
         <xs:enumeration value="S"/>
         <xs:enumeration value="B"/>
         <xs:enumeration value="H"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="AddrType">
      <xs:restriction base="xs:token">
         <xs:enumeration value="M"/>
         <xs:enumeration value="P"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="Addr">
      <xs:restriction base="xs:string">
		 <xs:minLength value="1"/>
         <xs:maxLength value="140"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="Suburb">
      <xs:restriction base="xs:token">
		 <xs:minLength value="1"/>
         <xs:maxLength value="35"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="State">
      <xs:restriction base="xs:token">
         <xs:minLength value="1"/>
		 <xs:maxLength value="35"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="StateOrEmpty">
      <xs:restriction base="xs:token">
         <xs:minLength value="0"/>
		 <xs:maxLength value="35"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="Postcode">
      <xs:restriction base="xs:token">
         <xs:minLength value="1"/>
		 <xs:maxLength value="15"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="PostcodeOrEmpty">
      <xs:restriction base="xs:token">
         <xs:minLength value="0"/>
		 <xs:maxLength value="15"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="Country">
      <xs:restriction base="xs:token">
         <xs:minLength value="3"/>
		 <xs:maxLength value="3"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:complexType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="Address">
      <xs:sequence>
         <xs:element name="addr" type="Addr" minOccurs="1"/>
         <xs:element name="suburb" type="Suburb" minOccurs="1"/>
         <xs:element name="state" type="State" minOccurs="1"/>
         <xs:element name="postcode" type="Postcode" minOccurs="1"/>
         <xs:element name="country" type="Country" minOccurs="1"/>
      </xs:sequence>
      <xs:attributeGroup ref="RequiredAttrs"/>
   </xs:complexType>
   <xs:complexType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="AddressBeneficiaryInstAndTransferee">
      <xs:sequence>
         <xs:element name="addr" type="Addr" minOccurs="1"/>
         <xs:element name="suburb" type="Suburb" minOccurs="1"/>
         <xs:element name="state" type="StateOrEmpty"/>
         <xs:element name="postcode" type="PostcodeOrEmpty"/>
         <xs:element name="country" type="Country" minOccurs="1"/>
      </xs:sequence>
      <xs:attributeGroup ref="RequiredAttrs"/>
   </xs:complexType>   
   <xs:complexType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="AddressFirma">
      <xs:sequence>
         <xs:element name="addr" fixed="Level 5, 2 Bulletin Place" minOccurs="1"/>
         <xs:element name="suburb" fixed="Sydney" minOccurs="1"/>
         <xs:element name="state" fixed="NSW" minOccurs="1"/>
         <xs:element name="postcode" fixed="2000" minOccurs="1"/>
         <xs:element name="country" fixed="AUS" minOccurs="1"/>
      </xs:sequence>
      <xs:attributeGroup ref="RequiredAttrs"/>
   </xs:complexType>
   <xs:complexType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="AddressBasic">
      <xs:sequence>
         <xs:element name="addr" type="Addr" minOccurs="1"/>
         <xs:element name="suburb" type="Suburb" minOccurs="1"/>
         <xs:element name="state" type="State" minOccurs="1"/>
         <xs:element name="postcode" type="Postcode" minOccurs="1"/>
         <xs:element name="country" type="Country" minOccurs="1"/>
      </xs:sequence>
      <xs:attributeGroup ref="RequiredAttrs"/>
   </xs:complexType>
   <xs:complexType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="AddressAllOptional">
      <xs:sequence>
         <xs:element name="addr" type="Addr" minOccurs="0"/>
         <xs:element name="suburb" type="Suburb" minOccurs="0"/>
         <xs:element name="state" type="State" minOccurs="0"/>
         <xs:element name="postcode" type="Postcode" minOccurs="0"/>
         <xs:element name="country" type="Country" minOccurs="0"/>
      </xs:sequence>
      <xs:attributeGroup ref="RequiredAttrs"/>
   </xs:complexType>
   <xs:complexType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="AddressWithType">
      <xs:sequence>
         <xs:element name="type" type="AddrType"/>
         <xs:element name="addr" type="Addr"/>
         <xs:element name="suburb" type="Suburb" minOccurs="0"/>
         <xs:element name="state" type="State" minOccurs="0"/>
         <xs:element name="postcode" type="Postcode" minOccurs="0"/>
         <xs:element name="country" type="Country"/>
      </xs:sequence>
      <xs:attributeGroup ref="RequiredAttrs"/>
   </xs:complexType>
   <xs:complexType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="AddressNoCountry">
      <xs:sequence>
         <xs:element name="addr" type="Addr"/>
         <xs:element name="suburb" type="Suburb"/>
         <xs:element name="state" type="State"/>
         <xs:element name="postcode" type="Postcode"/>
      </xs:sequence>
      <xs:attributeGroup ref="RequiredAttrs"/>
   </xs:complexType>
   <xs:simpleType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="AcctType">
      <xs:restriction base="xs:token">
         <xs:enumeration value="BETTING"/>
         <xs:enumeration value="BULLION"/>
         <xs:enumeration value="CHEQUE"/>
         <xs:enumeration value="CREDIT"/>
         <xs:enumeration value="CUSTODY"/>
         <xs:enumeration value="ECUR"/>
         <xs:enumeration value="FCUR"/>
         <xs:enumeration value="INS"/>
         <xs:enumeration value="INVEST"/>
         <xs:enumeration value="HIRE"/>
         <xs:enumeration value="LOAN"/>
         <xs:enumeration value="OTHER"/>
         <xs:enumeration value="PENSION"/>
         <xs:enumeration value="REMIT"/>
         <xs:enumeration value="RETIRE"/>
         <xs:enumeration value="VALCARD"/>
         <xs:enumeration value="SUPER"/>
         <xs:enumeration value="TRADE"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="AcctOtherDesc">
      <xs:restriction base="xs:token">
         <xs:maxLength value="20"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="AcctTitle">
      <xs:restriction base="xs:string">
         <xs:maxLength value="140"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="AcctBSB">
      <xs:restriction base="xs:token">
         <xs:pattern value="[0-9]{6}"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="AcctNumber">
      <xs:restriction base="xs:token">
         <xs:minLength value="1"/>
		 <xs:maxLength value="34"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:complexType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="AccountOptBSB">
      <xs:sequence>
         <xs:element name="title" type="AcctTitle"/>
         <xs:element name="bsb" type="AcctBSB" minOccurs="0"/>
         <xs:element name="number" type="AcctNumber"/>
      </xs:sequence>
      <xs:attributeGroup ref="RequiredAttrs"/>
   </xs:complexType>
   <xs:element xmlns:xhtml="http://www.w3.org/1999/xhtml" name="accountOptBSB"
               type="AccountOptBSB"/>
   <xs:complexType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="AccountAllOptional">
      <xs:sequence>
         <xs:element name="title" type="AcctTitle" minOccurs="0"/>
         <xs:element name="bsb" type="AcctBSB" minOccurs="0"/>
         <xs:element name="number" type="AcctNumber" minOccurs="0"/>
      </xs:sequence>
      <xs:attributeGroup ref="RequiredAttrs"/>
   </xs:complexType>
   <xs:element xmlns:xhtml="http://www.w3.org/1999/xhtml" name="accountAllOptional"
               type="AccountAllOptional"/>
   <xs:complexType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="AccountNoBSB">
      <xs:sequence>
         <xs:element name="title" type="AcctTitle"/>
         <xs:element name="number" type="AcctNumber"/>
      </xs:sequence>
      <xs:attributeGroup ref="RequiredAttrs"/>
   </xs:complexType>
   <xs:element xmlns:xhtml="http://www.w3.org/1999/xhtml" name="accountNoBSB"
               type="AccountNoBSB"/>
   <xs:complexType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="AccountNoTitle">
      <xs:sequence>
         <xs:element name="bsb" type="AcctBSB" minOccurs="0"/>
         <xs:element name="number" type="AcctNumber"/>
      </xs:sequence>
      <xs:attributeGroup ref="RequiredAttrs"/>
   </xs:complexType>
   <xs:complexType xmlns:xhtml="http://www.w3.org/1999/xhtml" name="AccountBrief">
      <xs:sequence>
         <xs:element name="bsb" type="AcctBSB" minOccurs="0"/>
         <xs:element name="number" type="AcctNumber" minOccurs="0"/>
      </xs:sequence>
      <xs:attributeGroup ref="RequiredAttrs"/>
   </xs:complexType>
